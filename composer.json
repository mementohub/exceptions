{
    "name": "imemento/exceptions",
    "description": "HTTP Exceptions",
    "homepage": "https://gitlab.com/imemento/composer/packages/exceptions",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Exceptions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Exceptions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
